<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="globParent" alignment="CENTER" fillWidth="false" minHeight="200.0" minWidth="200.0" prefHeight="350.0" prefWidth="350" style="-fx-background-image: url(net-wan-cloud.png);" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sc.cloud.client.LoginController">
   <Label text="Login@shamacloud" textFill="#0c20d7">
      <font>
         <Font name="Calibri Bold" size="24.0" />
      </font>
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin>
   </Label>
    <ComboBox fx:id="idLogin" blendMode="MULTIPLY" editable="true" onInputMethodTextChanged="#onKeyPresCBLog" onKeyPressed="#onKeyPresCBLog" onKeyReleased="#OnKeyReleasedCBLog" onKeyTyped="#onKeyPresCBLog" opacity="0.9" prefHeight="30.0" prefWidth="231.0" promptText="Login" visibleRowCount="3">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin></ComboBox>
    <PasswordField fx:id="idPassword" blendMode="MULTIPLY" onKeyReleased="#onKeyRelePassF" opacity="0.9" prefHeight="30.0" prefWidth="231.0" promptText="Password">
      <font>
         <Font size="14.0" />
      </font></PasswordField>
    <Button fx:id="idBtnGoIn" disable="true" onAction="#auth" prefHeight="25.0" prefWidth="93.0" text="Войти">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Button>
   <Hyperlink onAction="#onCheckIn" text="нету регистраций">
      <VBox.margin>
         <Insets top="50.0" />
      </VBox.margin>
   </Hyperlink>
   <padding>
      <Insets top="30.0" />
   </padding>
</VBox>
